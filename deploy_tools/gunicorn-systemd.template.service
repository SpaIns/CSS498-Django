[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=steffan
WorkingDirectory=/home/steffan/sites/SITENAME/source/superlists
ExecStart=/home/steffan/sites/SITENAME/virtualenv/bin/gunicorn --bind unix:/tmp/SITENAME.socket superlists.wsgi:application

[Install]
WantedBy=multi-user.target